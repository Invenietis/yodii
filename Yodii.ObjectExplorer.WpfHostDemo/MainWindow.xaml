        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ObjectExplorer demo host" Height="350" Width="525">
    <Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
        <StackPanel>
            <Button Click="Start_Click">Start engine</Button>
            <Button Click="Stop_Click">Stop engine</Button>
        </StackPanel>
    </Grid>
</Window>
